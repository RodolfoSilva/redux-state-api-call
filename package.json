    "jest": "^23.4.2",
    "npm-run-all": "^4.1.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "reselect": "^3.0.1",
    "rimraf": "^2.5.3",
    "webpack": "^4.16.5",
    "webpack-cli": "^2.0.11"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "lodash.memoize": "^4.1.2"
  },
  "peerDependencies": {
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0",
    "reselect": "^3.0.1"
